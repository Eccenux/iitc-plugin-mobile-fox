{
	"name": "iitc-plugin-mobile-fox",
	"version": "0.2.0",
	"description": "[0.2.0] Plugin focused on making IITC better for mobile phones. Mostly for users of Firefox for Android.",
	"author": "Eccenux",
	"license": "MIT",
	"main": "index.js",
	"monkeyscript": {
		"prependCSS": "src/css/index.css",
		"meta": {
			"name": "IITC plugin: Mobile Fox UX",
			"category": "Misc",
			"id": "iitc-plugin-mobile-fox@jonatkins",
			"namespace": "pl.enux.iitc",
			"updateUrl": "https://github.com/Eccenux/iitc-plugin-mobile-fox/raw/master/mobile-fox.meta.js",
			"downloadUrl": "https://github.com/Eccenux/iitc-plugin-mobile-fox/raw/master/mobile-fox.user.js",
			"grant": [
				"none"
			],
			"match": [
				"https://*.ingress.com/intel*",
				"http://*.ingress.com/intel*",
				"https://*.ingress.com/mission/*",
				"http://*.ingress.com/mission/*",
				"https://intel.ingress.com/*",
				"https://intel.ingress.com/*"
			]
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Eccenux/iitc-plugin-mobile-fox"
	},
	"devDependencies": {
		"eslint": "6.8.0",
		"gulp": "4.0.2",
		"gulp-concat": "2.6.1",
		"gulp-less": "4.0.1",
		"gulp-strip-comments": "2.5.2",
		"vinyl-source-stream": "2.0.0",
		"gulp-replace": "1.1.3",
		"gulp-monkeyscript": "file:../../gulp-monkeyscript"
	}
}
